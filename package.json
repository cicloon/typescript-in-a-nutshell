{
  "name": "typescrit-in-a-nutshell",
  "version": "1.0.0",
  "description": "Typescript examples",
  "main": "dist/index.js",
  "author": "cicloon",
  "license": "MIT",
  "scripts": {
    "dist": "npm run clean && tsc --emitDeclarationOnly && babel src --out-dir dist --extensions \".ts,.tsx\"",
    "dist:watch": "npm run clean && babel src --out-dir dist --extensions \".ts,.tsx\" --watch",
    "ts:check": "tsc --noEmit",
    "clean": "rm -f dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "prettier": "1.12.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@babel/preset-react": "7.0.0-beta.46",
    "@types/react": "^16.3.13",
    "@types/node": "^9.6.7",
    "@types/react-dom": "^16.0.5",
    "react": "^16.3.2"
  }
}
